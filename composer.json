{
    "name": "arousacode/webapp_liblet",
    "type": "library",
    "description": "Small library to automatically load data from HTMl form in any PHP object, and to generate HTML inputs from it. Also load and store into PDO database.",
    "homepage": "https://arousacode.github.io/WebAppLiblet/",
    "keywords": [
        "HTML",
        "Form",
        "Reflexion",
        "ORM",
        "PDO"
    ],
    "license": "GPLv3",
    "authors": [
        {
            "name": "Marcos MartÃ­nez",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "archive": {
        "exclude": [
            "/docs",
            "/test"
        ]
    },
    "autoload": {
        "psr-4": {
            "ArousaCode\\WebApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArousaCode\\WebApp\\Tests\\": "tests/"
        }
    }
}